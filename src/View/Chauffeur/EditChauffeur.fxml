      <TextField fx:id="lastNameTextBox" layoutX="38.0" layoutY="114.0" text="Last name" />
      <TextField fx:id="phoneNumberTextBox" layoutX="39.0" layoutY="161.0" text="Phone number" />
      <Text layoutX="390.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Chauffeur">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <Button layoutX="46.0" layoutY="324.0" mnemonicParsing="false" onAction="#backButtonPressed" text="Back" />
      <CheckBox fx:id="partyBusCheckBox" layoutX="37.0" layoutY="200.0" mnemonicParsing="false" text="Party Bus" />
      <CheckBox fx:id="schoolBusCheckBox" layoutX="38.0" layoutY="232.0" mnemonicParsing="false" text="School Bus" />
