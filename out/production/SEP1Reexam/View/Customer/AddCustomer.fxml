<AnchorPane prefHeight="430.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Customer.AddCustomerController">
   <children>
      <TextField fx:id="firstNameTextBox" layoutX="35.0" layoutY="66.0" promptText="First name" />
      <TextField fx:id="lastNameTextBox" layoutX="35.0" layoutY="119.0" promptText="Last name" />
      <TextField fx:id="emailTextBox" layoutX="36.0" layoutY="175.0" promptText="Email" />
      <TextField fx:id="phoneNumberTextBox" layoutX="36.0" layoutY="231.0" promptText="Phone number" />
      <Button layoutX="449.0" layoutY="345.0" mnemonicParsing="false" onAction="#saveCustomerButtonPressed" text="Save Customer" />
      <Button layoutX="35.0" layoutY="345.0" mnemonicParsing="false" onAction="#backButtonPressed" text="Back" />
      <Label fx:id="errorLabel" layoutX="416.0" layoutY="311.0" prefHeight="18.0" prefWidth="51.0" textFill="RED" />
      <Text layoutX="355.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Customer">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
   </children>
